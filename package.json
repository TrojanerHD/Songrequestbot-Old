{
  "name": "songrequestbot-twitch",
  "version": "0.0.5",
  "description": "A songrequestbot for Twitch providing YouTube and Spotify",
  "searchForSong": "./index.js",
  "scripts": {
    "debug": "node index.js",
    "pack": "electron-builder --dir",
    "dist": "rmdir /s /q dist&&electron-builder"
  },
  "author": "Trojaner",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "discord.js": "^11.5.1",
    "electron-updater": "^4.1.2",
    "express": "^4.16.4",
    "ffmpeg-binaries": "^4.0.0",
    "googleapis": "^42.0.0",
    "jquery": "^3.4.1",
    "jsdom": "^13.2.0",
    "opusscript": "^0.0.7",
    "request-promise": "^4.2.4",
    "tmi.js": "^1.3.1",
    "ytdl-core": "^1.0.0"
  },
  "build": {
    "productName": "Songrequestbot",
    "appId": "com.electron.songrequestbot",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "publish": [
        "github"
      ]
    }
  },
  "devDependencies": {
    "electron": "^6.0.4",
    "electron-builder": "^21.2.0"
  }
}
